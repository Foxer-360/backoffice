{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "Root",
  "required": [
    "id",
    "created",
    "updated",
    "fromDate",
    "toDate",
    "halfDay",
    "status"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "Id",
      "default": 0,
      "examples": [
        19557
      ]
    },
    "created": {
      "$id": "#/properties/created",
      "type": "object",
      "title": "Created",
      "required": [
        "date",
        "timezone_type",
        "timezone"
      ],
      "properties": {
        "date": {
          "$id": "#/properties/created/properties/date",
          "type": "string",
          "title": "Date",
          "default": "",
          "examples": [
            "2019-01-16 08:04:45.000000"
          ],
          "pattern": "^(.*)$"
        },
        "timezone_type": {
          "$id": "#/properties/created/properties/timezone_type",
          "type": "integer",
          "title": "Timezone_type",
          "default": 0,
          "examples": [
            3
          ]
        },
        "timezone": {
          "$id": "#/properties/created/properties/timezone",
          "type": "string",
          "title": "Timezone",
          "default": "",
          "examples": [
            "Europe/Prague"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "updated": {
      "$id": "#/properties/updated",
      "type": "object",
      "title": "Updated",
      "required": [
        "date",
        "timezone_type",
        "timezone"
      ],
      "properties": {
        "date": {
          "$id": "#/properties/updated/properties/date",
          "type": "string",
          "title": "Date",
          "default": "",
          "examples": [
            "2019-01-17 11:49:29.000000"
          ],
          "pattern": "^(.*)$"
        },
        "timezone_type": {
          "$id": "#/properties/updated/properties/timezone_type",
          "type": "integer",
          "title": "Timezone_type",
          "default": 0,
          "examples": [
            3
          ]
        },
        "timezone": {
          "$id": "#/properties/updated/properties/timezone",
          "type": "string",
          "title": "Timezone",
          "default": "",
          "examples": [
            "Europe/Prague"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "fromDate": {
      "$id": "#/properties/fromDate",
      "type": "object",
      "title": "Fromdate",
      "required": [
        "date",
        "timezone_type",
        "timezone"
      ],
      "properties": {
        "date": {
          "$id": "#/properties/fromDate/properties/date",
          "type": "string",
          "title": "Date",
          "default": "",
          "examples": [
            "2019-02-18 00:00:00.000000"
          ],
          "pattern": "^(.*)$"
        },
        "timezone_type": {
          "$id": "#/properties/fromDate/properties/timezone_type",
          "type": "integer",
          "title": "Timezone_type",
          "default": 0,
          "examples": [
            3
          ]
        },
        "timezone": {
          "$id": "#/properties/fromDate/properties/timezone",
          "type": "string",
          "title": "Timezone",
          "default": "",
          "examples": [
            "Europe/Prague"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "toDate": {
      "$id": "#/properties/toDate",
      "type": "object",
      "title": "Todate",
      "required": [
        "date",
        "timezone_type",
        "timezone"
      ],
      "properties": {
        "date": {
          "$id": "#/properties/toDate/properties/date",
          "type": "string",
          "title": "Date",
          "default": "",
          "examples": [
            "2019-02-21 00:00:00.000000"
          ],
          "pattern": "^(.*)$"
        },
        "timezone_type": {
          "$id": "#/properties/toDate/properties/timezone_type",
          "type": "integer",
          "title": "Timezone_type",
          "default": 0,
          "examples": [
            3
          ]
        },
        "timezone": {
          "$id": "#/properties/toDate/properties/timezone",
          "type": "string",
          "title": "Timezone",
          "default": "",
          "examples": [
            "Europe/Prague"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "halfDay": {
      "$id": "#/properties/halfDay",
      "type": "boolean",
      "title": "Halfday",
      "default": false,
      "examples": [
        false
      ]
    },
    "status": {
      "$id": "#/properties/status",
      "type": "object",
      "title": "Status",
      "required": [
        "id",
        "name",
        "const"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/status/properties/id",
          "type": "integer",
          "title": "Id",
          "default": 0,
          "examples": [
            2
          ]
        },
        "name": {
          "$id": "#/properties/status/properties/name",
          "type": "string",
          "title": "Name",
          "default": "",
          "examples": [
            "Schv√°leno"
          ],
          "pattern": "^(.*)$"
        },
        "const": {
          "$id": "#/properties/status/properties/const",
          "type": "string",
          "title": "Const",
          "default": "",
          "examples": [
            "approved"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}